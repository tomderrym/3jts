{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "contact-us-page-04",
  "type": "registry:block",
  "description": "Two-column contact layout with comprehensive form featuring service checkboxes, embedded Google Maps, and quick contact cards with avatar icons",
  "dependencies": [
    "lucide-react"
  ],
  "registryDependencies": [
    "avatar",
    "button",
    "card",
    "input",
    "label",
    "textarea",
    "checkbox"
  ],
  "files": [
    {
      "path": "src/registry/new-york/blocks/marketing-ui/contact-us-page/contact-us-page-04/page.tsx",
      "content": "import { MessageSquareMoreIcon, MessagesSquareIcon, MapPinIcon, PhoneIcon } from 'lucide-react'\n\nimport ContactUs from '@/components/shadcn-studio/blocks/contact-us-page-04/contact-us-page-04'\n\nconst contactCards = [\n  {\n    icon: MessageSquareMoreIcon,\n    title: 'Chat to Sales',\n    description: 'Speak to our friendly team',\n    ctaText: 'sales@gmail.com',\n    ctaLink: 'mailto:sales@gmail.com'\n  },\n  {\n    icon: MessagesSquareIcon,\n    title: 'Chat to Support',\n    description: \"We're here to help you\",\n    ctaText: 'johndoe@gmail.com',\n    ctaLink: 'mailto:johndoe@gmail.com'\n  },\n  {\n    icon: MapPinIcon,\n    title: 'Visit Us',\n    description: 'Visit our office',\n    ctaText: 'View on maps',\n    ctaLink: 'https://maps.google.com'\n  },\n  {\n    icon: PhoneIcon,\n    title: 'Call Us',\n    description: 'Mon to Fri from 8am to 5pm',\n    ctaText: '+124-2589-7854',\n    ctaLink: 'tel:+12425897854'\n  }\n]\n\nconst ContactUsPage = () => {\n  return <ContactUs contactCards={contactCards} />\n}\n\nexport default ContactUsPage\n",
      "type": "registry:page",
      "target": "app/contact-us-page-04/page.tsx"
    },
    {
      "path": "src/registry/new-york/components/blocks/contact-us-page-04/contact-form.tsx",
      "content": "'use client'\n\nimport { Button } from '@/components/ui/button'\nimport { Input } from '@/components/ui/input'\nimport { Label } from '@/components/ui/label'\nimport { Textarea } from '@/components/ui/textarea'\nimport { Checkbox } from '@/components/ui/checkbox'\n\nconst ContactForm = () => {\n  return (\n    <form className='space-y-6' onSubmit={e => e.preventDefault()}>\n      <div className='w-full space-y-2'>\n        <Label htmlFor='name'>Name</Label>\n        <Input type='text' id='name' className='h-10' placeholder='Enter your name here...' />\n      </div>\n\n      <div className='w-full space-y-2'>\n        <Label htmlFor='email'>Email</Label>\n        <Input type='email' id='email' className='h-10' placeholder='Enter your Email here...' />\n      </div>\n\n      <div className='w-full space-y-2'>\n        <Label htmlFor='message'>Message</Label>\n        <Textarea id='message' className='h-28 resize-none' placeholder='Type here' />\n      </div>\n\n      {/* Services Checkboxes */}\n      <div>\n        <p className='mb-4 font-medium'>Services</p>\n        <div className='grid gap-x-10 gap-y-4 sm:grid-cols-2'>\n          <div className='flex items-center gap-3'>\n            <Checkbox className='size-6' id='websitedesign' />\n            <Label htmlFor='websitedesign' className='text-sm'>\n              Website Design\n            </Label>\n          </div>\n\n          <div className='flex items-center gap-3'>\n            <Checkbox className='size-6' id='contentcreation' />\n            <Label htmlFor='contentcreation' className='text-sm'>\n              Content Creation\n            </Label>\n          </div>\n\n          <div className='flex items-center gap-3'>\n            <Checkbox className='size-6' id='ux-design' />\n            <Label htmlFor='ux-design' className='text-sm'>\n              UX Design\n            </Label>\n          </div>\n\n          <div className='flex items-center gap-3'>\n            <Checkbox className='size-6' id='strategy-consulting' />\n            <Label htmlFor='strategy-consulting' className='text-sm'>\n              Strategy & Consulting\n            </Label>\n          </div>\n        </div>\n      </div>\n\n      <Button type='submit' className='w-full rounded-lg text-base shadow-sm' size='lg'>\n        Send Message\n      </Button>\n    </form>\n  )\n}\n\nexport default ContactForm\n",
      "type": "registry:component",
      "target": "components/shadcn-studio/blocks/contact-us-page-04/contact-form.tsx"
    },
    {
      "path": "src/registry/new-york/components/blocks/contact-us-page-04/contact-us-page-04.tsx",
      "content": "import type { ComponentType } from 'react'\n\nimport { Avatar, AvatarFallback } from '@/components/ui/avatar'\nimport { Button } from '@/components/ui/button'\nimport { Card, CardContent } from '@/components/ui/card'\n\nimport ContactForm from '@/components/shadcn-studio/blocks/contact-us-page-04/contact-form'\n\ntype ContactCard = {\n  icon: ComponentType\n  title: string\n  description: string\n  ctaText: string\n  ctaLink: string\n}[]\n\nconst ContactUs = ({ contactCards }: { contactCards: ContactCard }) => {\n  return (\n    <section className='bg-muted py-8 sm:py-16 lg:py-24'>\n      <div className='mx-auto max-w-7xl px-4 sm:px-6 lg:px-8'>\n        <div className='mb-12 text-center sm:mb-16 lg:mb-24'>\n          <h2 className='mb-4 text-2xl font-semibold md:text-3xl lg:text-4xl'>Stay Connected with Us</h2>\n          <p className='text-muted-foreground text-xl'>\n            Reach out for inquiries, support, or collaborationâ€”we&apos;d love to hear from you!\n          </p>\n        </div>\n\n        <Card className='shadow-none'>\n          <CardContent className='grid gap-6 md:grid-cols-2'>\n            <ContactForm />\n\n            {/* Map Section */}\n            <div>\n              <iframe\n                className='size-full min-h-[400px] rounded-md'\n                src='https://maps.google.com/maps?hl=en&amp;q=%20new%20york+(New%20york)&amp;t=&amp;z=12&amp;ie=UTF8&amp;iwloc=B&amp;output=embed'\n                title='Google Maps'\n              />\n            </div>\n          </CardContent>\n        </Card>\n\n        {/* Contact Cards */}\n        <div className='mt-10 grid gap-6 md:grid-cols-2 xl:grid-cols-4'>\n          {contactCards.map((contact, index) => (\n            <Card\n              key={index}\n              className='hover:bg-primary hover:border-primary group shadow-none transition-all duration-300'\n            >\n              <CardContent className='flex flex-col gap-6'>\n                <Avatar className='size-11.5 rounded-lg'>\n                  <AvatarFallback className='bg-primary/10 group-hover:bg-primary-foreground group-hover:text-primary rounded-lg transition-all duration-300 [&>svg]:size-7'>\n                    <contact.icon />\n                  </AvatarFallback>\n                </Avatar>\n                <div className='space-y-2'>\n                  <h3 className='group-hover:text-primary-foreground text-lg font-semibold transition-all duration-300'>\n                    {contact.title}\n                  </h3>\n                  <p className='text-muted-foreground group-hover:text-primary-foreground transition-all duration-300'>\n                    {contact.description}\n                  </p>\n                </div>\n                <Button\n                  className='bg-primary/10 text-primary hover:bg-primary/20 focus-visible:ring-primary/20 dark:focus-visible:ring-primary/40 group-hover:bg-secondary group-hover:text-secondary-foreground group-hover:hover:bg-secondary text-base shadow-none group-hover:shadow-sm'\n                  size='lg'\n                >\n                  <a href={contact.ctaLink}>{contact.ctaText}</a>\n                </Button>\n              </CardContent>\n            </Card>\n          ))}\n        </div>\n      </div>\n    </section>\n  )\n}\n\nexport default ContactUs\n",
      "type": "registry:component",
      "target": "components/shadcn-studio/blocks/contact-us-page-04/contact-us-page-04.tsx"
    }
  ]
}