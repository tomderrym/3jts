  "files": [
    {
      "path": "src/registry/new-york/blocks/marketing-ui/navbar-component/navbar-component-09/page.tsx",
      "content": "import Navbar from '@/components/shadcn-studio/blocks/navbar-component-09/navbar-component-09'\n\nconst navigationItems = [\n  {\n    title: 'Home',\n    href: '#'\n  },\n  {\n    title: 'Products',\n    href: '#',\n    isActive: true\n  },\n  {\n    title: 'About us',\n    href: '#'\n  },\n  {\n    title: 'Pricing',\n    href: '#'\n  }\n]\n\nconst NavbarPage = () => {\n  return (\n    <div className='h-140'>\n      <Navbar navigationData={navigationItems} />\n    </div>\n  )\n}\n\nexport default NavbarPage\n",
      "type": "registry:page",
      "target": "app/navbar-component-09/page.tsx"
    },
    {
