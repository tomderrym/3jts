  "files": [
    {
      "path": "src/registry/new-york/blocks/ecommerce/category-filter/category-filter-05/page.tsx",
      "content": "import CategoryFilter from '@/components/shadcn-studio/blocks/category-filter-05/category-filter-05'\n\nconst CategoryFilterPage = () => {\n  return <CategoryFilter />\n}\n\nexport default CategoryFilterPage\n",
