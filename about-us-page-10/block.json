{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "about-us-page-10",
  "type": "registry:block",
  "description": "Two-column layout with decorative dots pattern, image section and achievements list with check icons and statistics",
  "dependencies": [
    "lucide-react"
  ],
  "registryDependencies": [
    "badge",
    "button",
    "card"
  ],
  "files": [
    {
      "path": "src/registry/new-york/blocks/marketing-ui/about-us-page/about-us-page-10/page.tsx",
      "content": "import AboutUs from '@/components/shadcn-studio/blocks/about-us-page-10/about-us-page-10'\n\nconst achievementsList = [\n  { number: '20+', description: 'Years of Valuable Experience' },\n  { number: '70+', description: 'Successful Projects and Initiatives' },\n  { number: '85+', description: 'Trusted Employees Network' },\n  { number: '35+', description: 'Positive Customer Reviews' },\n  { number: '15', description: 'Achieve Recognition and Awards' }\n]\n\nconst AboutUsPage = () => {\n  return <AboutUs achievementsList={achievementsList} />\n}\n\nexport default AboutUsPage\n",
      "type": "registry:page",
      "target": "app/about-us-page-10/page.tsx"
    },
    {
      "path": "src/registry/new-york/components/blocks/about-us-page-10/about-us-page-10.tsx",
      "content": "import { CircleCheckBigIcon } from 'lucide-react'\n\nimport { Badge } from '@/components/ui/badge'\nimport { Button } from '@/components/ui/button'\nimport { Card, CardContent } from '@/components/ui/card'\n\ntype Achievements = {\n  number: string\n  description: string\n}[]\n\nconst AboutUs = ({ achievementsList }: { achievementsList: Achievements }) => {\n  return (\n    <section className='bg-muted py-8 sm:py-16 lg:py-24'>\n      <div className='mx-auto max-w-7xl px-4 sm:px-6 lg:px-8'>\n        {/* Header */}\n        <div className='mb-12 flex items-center justify-between gap-6 max-lg:flex-col md:mb-16 lg:mb-24'>\n          <div className='space-y-4'>\n            <Badge variant='outline' className='rounded-full text-sm font-normal'>\n              About Us\n            </Badge>\n            <h2 className='text-2xl font-semibold md:text-3xl lg:text-4xl'>Our Story & Mission</h2>\n            <p className='text-muted-foreground text-xl'>\n              Built on innovation, integrity, and excellence, we strive to create meaningful impact through our work.\n            </p>\n          </div>\n          <Button asChild className='rounded-lg text-base shadow-sm' size='lg'>\n            <a href='#'>Read More</a>\n          </Button>\n        </div>\n\n        <div className='grid gap-16 lg:grid-cols-2'>\n          {/* Image container */}\n          <div className='relative z-1 h-max w-full'>\n            {/* Decorative dots */}\n            <svg\n              width='51'\n              height='57'\n              viewBox='0 0 51 57'\n              fill='none'\n              xmlns='http://www.w3.org/2000/svg'\n              className='absolute -top-4 -left-6 -z-1 opacity-20 max-lg:hidden'\n            >\n              <g>\n                {/* Primary colored dots pattern */}\n                {Array.from({ length: 8 }).map((_, row) =>\n                  Array.from({ length: 7 }).map((_, col) => (\n                    <ellipse\n                      key={`${row}-${col}`}\n                      cx='2.5'\n                      cy='2.5'\n                      rx='2.5'\n                      ry='2.5'\n                      transform={`translate(${5 + col * 7}, ${4 + row * 6})`}\n                      fill='var(--primary)'\n                      fillOpacity='0.5'\n                    />\n                  ))\n                )}\n              </g>\n            </svg>\n\n            <img\n              src='https://cdn.shadcnstudio.com/ss-assets/blocks/marketing/about-us/image-1.png'\n              alt='About us image'\n              className='h-91 w-full rounded-lg object-cover'\n            />\n\n            {/* Decorative dots */}\n            <svg\n              width='51'\n              height='57'\n              viewBox='0 0 51 57'\n              fill='none'\n              xmlns='http://www.w3.org/2000/svg'\n              className='absolute -right-8 -bottom-6 -z-1 opacity-20 max-lg:hidden'\n            >\n              <g>\n                {/* Destructive colored dots pattern */}\n                {Array.from({ length: 8 }).map((_, row) =>\n                  Array.from({ length: 7 }).map((_, col) => (\n                    <ellipse\n                      key={`${row}-${col}`}\n                      cx='2.5'\n                      cy='2.5'\n                      rx='2.5'\n                      ry='2.5'\n                      transform={`translate(${5 + col * 7}, ${4 + row * 6})`}\n                      fill='var(--destructive)'\n                      fillOpacity='0.5'\n                    />\n                  ))\n                )}\n              </g>\n            </svg>\n          </div>\n\n          {/* Stats */}\n          <div className='grid grid-cols-1 gap-4 md:max-lg:grid-cols-2'>\n            {achievementsList.map((achievement, index) => (\n              <Card\n                key={index}\n                className='rounded-lg border-none py-4 shadow-none transition-shadow duration-300 hover:shadow-sm last:max-lg:col-span-full'\n              >\n                <CardContent>\n                  <div className='flex items-center gap-3'>\n                    <CircleCheckBigIcon />\n                    <span className='flex items-center gap-2.5'>\n                      <span className='text-xl font-semibold'>{achievement.number}</span>\n                      <span className='text-muted-foreground font-medium'>{achievement.description}</span>\n                    </span>\n                  </div>\n                </CardContent>\n              </Card>\n            ))}\n          </div>\n        </div>\n      </div>\n    </section>\n  )\n}\n\nexport default AboutUs\n",
      "type": "registry:component",
      "target": "components/shadcn-studio/blocks/about-us-page-10/about-us-page-10.tsx"
    }
  ]
}